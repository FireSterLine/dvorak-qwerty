partial default xkb_types "default" {
    virtual_modifiers Meta,Alt,Super,NumLock,LevelFive;

    type "DVORAK_QWERTY" {
        modifiers = None+Shift+Lock+Control+Alt+Super+NumLock+LevelFive;
        map[None]                  = level1;
        map[Shift]                 = level2;
        map[Shift+Control]         = Level2;
        map[Shift+Alt]             = Level2;
        map[Shift+Super]           = Level2;

        map[NumLock]               = level3;
        map[Shift+NumLock]         = level4;

        map[Lock+NumLock]          = Level5;
        map[Shift+Lock+NumLock]    = Level6;

        map[LevelFive]             = Level7;
        map[Shift+LevelFive]       = Level8;

        level_name[Level1]= "Qwerty mods";
        level_name[Level2]= "Qwerty mods + Shift";
        level_name[Level3]= "Base";
        level_name[Level4]= "Shift";
        level_name[Level5]= "Caps";
        level_name[Level6]= "Caps + Shift";
        level_name[Level7]= "LevelFive";
        level_name[Level8]= "Shift+LevelFive";
    };

};
